  "repository": {
    "type": "git",
    "url": "https://github.deere.com/TeamManager/team-manager-app.git"
  },
  "private": true,
  "scripts": {
    "analyze:ui:open": "open webpack-analysis.html",
    "build": "npm run build:ui && npm run build:server && npm run build:error-handler",
    "build:error-handler": "hapi-error-handling copy assets",
    "build:server": "babel src --out-dir lib --ignore \"src/ui-core\"",
    "build:ui": "cross-env FORCE_COLOR=true build-tasks js --clean false",
    "build:ui:local": "cross-env FORCE_COLOR=true build-tasks js --clean false --config webpack.config.local.js",
    "build:views:local": "npm run build:ui:local && babel src --out-dir lib --only **/views/** && npm run build:error-handler",
    "build:zip": "bundle-dependencies update && npm-pack-zip",
    "ci:build": "npm run build && npm run translations:pull && npm run build:zip",
    "clean": "npm run clean:tests && npm run clean:built",
    "clean:built": "rimraf lib assets/error assets/js assets/css assets/translations assets/config assets/index.html assets/favicon.ico",
    "clean:cache": "rimraf node_modules/.cache",
    "clean:tests": "rimraf test/acceptance/cypress/screenshots hs_err_*.log",
    "coverage:report": "open ./coverage/lcov-report/index.html",
    "dependencies:audit": "pkg-audit --threshold critical",
    "encrypt:qual": "cross-env AWS_PROFILE=qual node scripts/encrypt",
    "encrypt:cert": "cross-env AWS_PROFILE=cert node scripts/encrypt",
    "encrypt:prod": "cross-env AWS_PROFILE=prod node scripts/encrypt",
    "license": "build-tasks license",
    "lint": "concurrently --raw npm:lint:* npm:typecheck",
    "fixlint": "build-tasks eslint --fix",
    "lint:features": "cross-env FORCE_COLOR=true build-tasks eslint --files test/acceptance --extensions '.feature'",
    "lint:js": "cross-env FORCE_COLOR=true build-tasks eslint",
    "lint:styles": "cross-env FORCE_COLOR=true build-tasks stylelint",
    "routes": "babel-node ./scripts/show-routes.js",
    "start": "node ./lib/team-manager-server.js",
    "start-acceptance": "npm run clean && cross-env NODE_ENV=acceptance npm run build:views:local && cross-env NODE_ENV=acceptance LOG_LEVEL=info APP_URL=http://127.0.0.1:8081 PLATFORM_PREFIX=http://127.0.0.1:8083 PERSISTENT_HEADER_URL=http://127.0.0.1:8084 APP_PORT=8081 HOT=true CYPRESS=true babel-watch --inspect=9222 --exclude ./test/acceptance/cypress -- ./test/acceptance/server.js",
    "start-cypress": "concurrently --raw \"npm run start-acceptance\" \"node scripts/open-cypress\"",
    "start-fake-contract-store": "docker compose up --build -d contract-store",
    "start-local": "npm run start-redis && npm run start-fake-contract-store && npm run clean && cross-env NODE_ENV=localhost npm run build:views:local && cross-env NODE_ENV=localhost babel-watch --inspect=9222 -- ./localhost/server-core/index.js",
    "start-local-docker": "npm run build && npm run translations:pull && docker build -t team-manager . && docker run --rm -e NODE_ENV=localhost --env-file .env -v $(pwd)/assets:/home/node/assets -p 8081:8081 team-manager",
    "start-qual": "npm run start-redis && npm run clean && npm run build && cross-env NODE_ENV=localhost npm run start",
    "start-redis": "docker compose up -d redis",
    "test": "cross-env ALLOW_CONFIG_MUTATIONS=1 FORCE_COLOR=true DEBUG_PRINT_LIMIT=10000 NODE_ENV=test ts-mocha test/unit",
    "test:acceptance": "npm run clean:tests && cross-env CYPRESS=true CYPRESS_RETRIES=5 node scripts/run-cypress.js",
    "test:coverage": "cross-env FORCE_COLOR=true build-tasks coverage --testCmd \"npm run test\"",
    "test:server-startup": "npm prune --loglevel=error --production && LOG_LEVEL=info node scripts/run-startup-tests.js",
    "test:watch": "cross-env ALLOW_CONFIG_MUTATIONS=1 FORCE_COLOR=true DEBUG_PRINT_LIMIT=10000 NODE_ENV=test ts-mocha test/unit -w --watch-files '**/*.*'",
    "translations:diff": "babel-node ./scripts/translations-diff.js",
    "translations:pull": "translations-downloader",
    "typecheck": "tsc -p tsconfig.json --checkJs false --pretty",
    "verify": "npm run clean && npm run lint && npm run dependencies:audit && build-tasks license && npm run test:coverage && npm run build && npm run test:acceptance"
  },
